@misc{thollet_observatoire_2024,
	title = {Observatoire des Sédiments du Rhône},
	url = {https://entrepot.recherche.data.gouv.fr/citation?persistentId=doi:10.17180/obs.osr},
	doi = {10.17180/OBS.OSR},
	abstract = {L’Observatoire des Sédiments du Rhône ({OSR}) a été créé en 2009 à la suite de questions qui ont émergé dans le cadre du plan Rhône. Sur le Rhône, du Léman à la Méditerranée, soit un linéaire de plus de 500 km, cet observatoire a pour mission de produire, rassembler et gérer des données visant à caractériser les stocks et les flux sédimentaires, ainsi que les pollutions associées à ces sédiments.L’{OSR} est un programme de recherche financé au titre du Plan Rhône et bénéficie du soutien du Fond Européen pour le Développement Régional. L’{OSR} est un programme de recherche regroupant scientifiques ({CNRS}, {INRAE}, {ENTPE}, {IRSN}, Ifremer) et les principaux gestionnaires du fleuve ({DREAL}, Agence de l’Eau, la {CNR}, les régions Auvergne Rhône-Alpes, Provence-Alpes-Côte d’Azur et Occitanie, et {EDF}). Il constitue l’un des observatoires de la Zone Atelier du Bassin du Rhône ({ZABR}).Sites {expérimentauxRéseau} de mesure des flux de l'{OSRCe} site expérimental regroupe les suivis par les différents partenaires de l'{OSR} des débits, des concentrations en matières en suspension et des concentrations en micropolluants sur le Rhône et ses affluents.},
	publisher = {Recherche Data Gouv},
	author = {Thollet, Fabien and Le Bescond, Chloé and Lagouy, Mickaël and Gruat, Alexandra and Grisot, Ghislaine and Le Coz, Jérôme and Coquery, Marina and Lepage, Hugo and Gairoard, Stéphanie and Gattacceca, Julie Claire and Ambrosi, Jean-Paul and Radakovitch, Olivier and Dur, Geneviève and Richard, Loic and Giner, F. and Eyrolle, Frédérique and Angot, Hélène and Mourier, D. and Bonnefoy, Adrien and Dugué, Violaine and Launay, Marina and Troudet, Laura and Labille, Jérôme and Kieffer, Léa and {Union Européenne} and {Métropole De Lyon} and {Institut Méditerranéen D'océanologie} and {Fonds Européen De Développement Régional} and {Institut Français De Recherche Pour L'exploitation De La Mer} and {Électricité De France} and {Compagnie Nationale Du Rhône} and {Institut De Radioprotection Et De Sûreté Nucléaire} and {Centre De Recherche Et D’enseignement Des Géosciences De L’environnement} and {Zone Atelier Bassin Du Rhône} and {Services De Prévision Des Crues Du Grand Delta} and {RiverLy} and Direction Régionale De L'environnement, De L'aménagement Et Du Logement De L'Auvergne-Rhône-Alpes and {Office Fédéral De L'environnement}},
